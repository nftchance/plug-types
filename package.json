{
  "name": "@nftchance/plug-types",
  "version": "0.10.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nftchance/plug-types.git"
  },
  "keywords": [
    "nftchance",
    "eip712",
    "typehash",
    "signatures",
    "eth",
    "ethereum",
    "solidity",
    "crypto",
    "intent",
    "plugs",
    "plug"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "DEV=true tsup",
    "prebuild": "tsup && pnpm plug zod && pnpm lint",
    "build": "tsup && pnpm plug generate && pnpm plug docs && forge fmt dist/contracts/Types.sol",
    "plug": "node dist/core/cli.mjs",
    "lint": "tsc --noEmit",
    "format": "pnpm prettier --write ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "abitype": "^1.0.5",
    "bundle-require": "^5.0.0",
    "commander": "^12.1.0",
    "dedent": "^1.5.3",
    "ethers": "^6.13.2",
    "execa": "^9.3.0",
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "pathe": "^1.1.2",
    "picocolors": "^1.0.1",
    "prettier": "^3.3.3",
    "solady": "^0.0.232",
    "tsx": "^4.16.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.6",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.14.9",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "bin": {
    "plug": "dist/core/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core/cli": {
      "types": "./dist/core/cli.d.ts",
      "default": "./dist/core/cli.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/core",
    "/lib",
    "/dist"
  ]
}
